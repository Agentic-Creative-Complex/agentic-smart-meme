{
  "name": "meme-agent-api",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run migrate && npm run seed && node dist/index.js",
    "migrate": "npx ts-node ./node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "npx ts-node ./node_modules/.bin/sequelize db:migrate:undo",
    "seed": "npx ts-node ./node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "npx ts-node ./node_modules/.bin/sequelize db:seed:undo",
    "dev": "npm run migrate && npm run seed && nodemon --watch 'src/**/*' -e ts,tsx --exec 'node --inspect=0.0.0.0:9233 -r ts-node/register ./src/index.ts'",
    "build": "npm test && tsc",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@slack/web-api": "^7.8.0",
    "@types/express": "^4.17.17",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.0.4",
    "fs": "^0.0.1-security",
    "install": "^0.13.0",
    "npm": "^10.2.5",
    "oauth-1.0a": "^2.2.6",
    "openai": "^4.72.0",
    "path": "^0.12.7",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "process": "^0.11.10",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "sequelize-typescript": "^2.1.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "telegraf": "^4.16.3",
    "twitter-api-v2": "^1.18.2",
    "uuid": "^9.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.17.9",
    "@types/sequelize": "^4.28.15",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.8",
    "@types/winston": "^2.4.4",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
